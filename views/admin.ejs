<!DOCTYPE html>
<html lang="en">
    <head>
        <title></title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <style> 
            table, th, td {
                border: 1px solid black;
                border-collapse: collapse;
                padding: 10px;
            }
        </style>   
    </head>
    <body>
        <table>
            <tr>
                <td>STT</td>
                <td>username</td>
                <td>password</td>
            </tr>
            <% arrUsers.forEach((e, i) => { %>
                <tr>
                    <td><%= i + 1 %></td>
                    <td><%= e.username %></td>
                    <td><%= e.password %></td>
                </tr>
            <% }); %>
        </table>
    </body>
</html>